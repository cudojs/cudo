{
  "name": "cudo",
  "version": "0.7.3",
  "repository": "cudojs/cudo",
  "description": "Cudo microservices framework",
  "keywords": [
    "cudo",
    "framework",
    "microservices"
  ],
  "license": "BSD-2-Clause",
  "author": {
    "name": "Marek Maciusowicz",
    "email": "m.maciusowicz@cudojs.com",
    "url": "http://www.cudojs.com"
  },
  "engines": {
    "node": ">=8.11.1"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^2.2.48",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "source-map-support": "^0.5.5",
    "ts-node": "^6.0.2",
    "typescript": "^2.8.3"
  },
  "scripts": {
    "build": "npm run test && tsc",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  }
